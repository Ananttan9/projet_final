
<%- include('header.ejs', {name : undefined})%>

<body>
  <div class="container">

    <header>
      <h2 class="Titre">Inscription</h2>
    </header>
    <form class="Formulaire" action="/Inscription" method="post">
      <div class="form-group">
        <label for="name">Nom</label>
        <input type="text" name="user_name" class="form-control" id="name" aria-describedby="emailHelp">
      </div>
      <div class="form-group">
        <label for="mail">Adresse email</label>
        <input type="email" name="user_email" class="form-control" id="mail" aria-describedby="emailHelp">
      </div>
      <div class="form-group">
        <label for="Mdp">Mot de passe</label>
        <input type="password" type="password" id="Mdp" name="Mot_de_passe" class="form-control">
      </div>
      <button type="submit" class="btn btn-primary">Envoyer</button>
    </form>
  </div>
